  "author": "Connor Burns <ctrekker4@gmail.com>",
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.6.4",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-link": "^2.4.13",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.13",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19",
    "typography-theme-lincoln": "^0.16.19"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build --prefix-paths && rm public/*.js.map",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build --prefix-paths && echo cotangent.software > public/CNAME && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cotangent-software/cotangent.software"
  },
  "bugs": {
    "url": "https://github.com/cotangent-software/cotangent.software/issues"
  }
